package Viikko4;

/**
 *
 * @author Miko
 */
public class Henkilo {
    
String etunimi, sukunimi;



public Henkilo (String etunimi,String sukunimi){
this.etunimi = etunimi;
this.sukunimi = sukunimi;

}
 

public String toString( ){

   String nimi = etunimi + " " + sukunimi; 
    return nimi;
}    
 
}
